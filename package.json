{
	"name": "log-analysis-plus",
	"displayName": "Log Analysis+",
	"description": "Advanced filtering and highlighting capabilities for more efficient log analysis",
	"publisher": "JeanTracker",
	"repository": {
		"type": "git",
		"url": "https://github.com/JeanTracker/log-analysis-plus.git"
	},
	"version": "1.1.0",
	"engines": {
		"vscode": "^1.49.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "log-analysis-plus.addGroup",
				"title": "Add Group",
				"icon": "$(file-directory-create)"
			},
			{
				"command": "log-analysis-plus.editGroup",
				"title": "Edit a new name for This Group",
				"icon": "$(edit)"
			},
			{
				"command": "log-analysis-plus.deleteGroup",
				"title": "Delete This Group",
				"icon": "$(dialog-close)"
			},
			{
				"command": "log-analysis-plus.enableHighlight",
				"title": "Enable Highlight",
				"icon": "$(paintcan)"
			},
			{
				"command": "log-analysis-plus.disableHighlight",
				"title": "Disable Highlight",
				"icon": "$(paintcan)"
			},
			{
				"command": "log-analysis-plus.enableVisibility",
				"title": "Enable Visbility",
				"icon": "$(eye)"
			},
			{
				"command": "log-analysis-plus.disableVisibility",
				"title": "Disable Visbility",
				"icon": "$(eye-closed)"
			},
			{
				"command": "log-analysis-plus.turnOnFocusMode",
				"title": "Turn on Focus Mode",
				"icon": "$(symbol-keyword)"
			},
			{
				"command": "log-analysis-plus.deleteFilter",
				"title": "Delete This Filter",
				"icon": "$(dialog-close)"
			},
			{
				"command": "log-analysis-plus.editFilter",
				"title": "Edit Regex for This Filter",
				"icon": "$(edit)"
			},
			{
				"command": "log-analysis-plus.addFilter",
				"title": "Add a Filter",
				"icon": "$(plus)"
			},
			{
				"command": "log-analysis-plus.deleteProject",
				"title": "Delete This Project",
				"icon": "$(dialog-close)"
			},
			{
				"command": "log-analysis-plus.addProject",
				"title": "Add Project",
				"icon": "$(repo-create)"
			},
			{
				"command": "log-analysis-plus.openSettings",
				"title": "Open Settings",
				"icon": "$(gear)"
			},
			{
				"command": "log-analysis-plus.refreshSettings",
				"title": "Refresh",
				"icon": "$(refresh)"
			},
			{
				"command": "log-analysis-plus.saveProject",
				"title": "Save groups to project",
				"icon": "$(save-as)"
			},
			{
				"command": "log-analysis-plus.selectProject",
				"title": "Select project"
			}
		],
		"menus": {
			"view/item/context": [
				{
					"command": "log-analysis-plus.addFilter",
					"when": "view == filters.plus && viewItem =~ /^g-/",
					"group": "inline@1"
				},
				{
					"command": "log-analysis-plus.editGroup",
					"when": "view == filters.plus && viewItem =~ /^g-/",
					"group": "inline@2"
				},
				{
					"command": "log-analysis-plus.editFilter",
					"when": "view == filters.plus && viewItem =~ /^f-/",
					"group": "inline@2"
				},
				{
					"command": "log-analysis-plus.disableHighlight",
					"when": "view == filters.plus && viewItem =~ /-lit/",
					"group": "inline@3"
				},
				{
					"command": "log-analysis-plus.enableHighlight",
					"when": "view == filters.plus && viewItem =~ /-unlit/",
					"group": "inline@3"
				},
				{
					"command": "log-analysis-plus.disableVisibility",
					"when": "view == filters.plus && viewItem =~ /-visible/",
					"group": "inline@4"
				},
				{
					"command": "log-analysis-plus.enableVisibility",
					"when": "view == filters.plus && viewItem =~ /-invisible/",
					"group": "inline@4"
				},
				{
					"command": "log-analysis-plus.deleteGroup",
					"when": "view == filters.plus && viewItem =~ /^g-/",
					"group": "inline@5"
				},
				{
					"command": "log-analysis-plus.deleteFilter",
					"when": "view == filters.plus && viewItem =~ /^f-/",
					"group": "inline@5"
				},
				{
					"command": "log-analysis-plus.deleteProject",
					"when": "view == filters.plus.settings",
					"group": "inline@5"
				}
			],
			"view/title": [
				{
					"command": "log-analysis-plus.addGroup",
					"when": "view == filters.plus",
					"group": "navigation@1"
				},
				{
					"command": "log-analysis-plus.turnOnFocusMode",
					"when": "view == filters.plus",
					"group": "navigation@2"
				},
				{
					"command": "log-analysis-plus.saveProject",
					"when": "view == filters.plus",
					"group": "navigation@3"
				},
				{
					"command": "log-analysis-plus.addProject",
					"when": "view == filters.plus.settings",
					"group": "navigation@1"
				},
				{
					"command": "log-analysis-plus.openSettings",
					"when": "view == filters.plus.settings",
					"group": "navigation@2"
				},
				{
					"command": "log-analysis-plus.refreshSettings",
					"when": "view == filters.plus.settings",
					"group": "navigation@3"
				}
			]
		},
		"keybindings": [
			{
				"command": "log-analysis-plus.turnOnFocusMode",
				"key": "ctrl+h",
				"mac": "cmd+h"
			}
		],
		"views": {
			"explorer": [
				{
					"id": "filters.plus",
					"name": "Filters+"
				}
			],
			"filter_group_setting": [
				{
					"id": "filters.plus.settings",
					"name": "Projects"
				}
			]
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "filter_group_setting",
					"title": "Log Analysis+",
					"icon": "$(preview)"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.49.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"eslint": "^7.6.0",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	}
}